
rm -rf CMake* 

TRILINSTALLDIR=/home/ikalash/Trilinos/build/install 

cmake \
    -D ALBANY_TRILINOS_DIR:PATH=${TRILINSTALLDIR} \
    -D ENABLE_ALBANY_EPETRA:BOOL=ON \
    -D ENABLE_CHECK_FPE:BOOL=OFF \
    -D SEACAS_EPU=${TRILINSTALLDIR}/bin/epu \
    -D SEACAS_DECOMP=${TRILINSTALLDIR}/bin/decomp \
    -D SEACAS_EXODIFF=${TRILINSTALLDIR}/bin/exodiff \
    -D SEACAS_ALGEBRA=${TRILINSTALLDIR}/bin/algebra \
..
